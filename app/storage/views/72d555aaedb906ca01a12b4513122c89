				<?php if((bool)$image->PhotoGalleryAlbumId): ?>
					data-ngalbumid = "<?php echo $image->PhotoGalleryAlbumId; ?>"
				<?php endif; ?>
				data-ngdesc="<?php echo $image->ImageDescription; ?>"><?php echo $image->ImageName; ?></a>
